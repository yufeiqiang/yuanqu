page{
  background: #fff;
}
.title_box{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}
.title_visitor{
  justify-content: center;
  border-top:1px solid #ededed;
  padding: 0; 
}
.title_visitor view{
  width: 50%;
  padding: 26rpx 0;
  text-align: center;
  border-bottom: 1px solid #ededed;
}
.title_visitor view:nth-of-type(1){
  border-right: 1px solid #ededed;
}
.item_null{
  height: 120rpx;
  width: 100%;
}
.title_visitor view.title_visitor_curr{
  color: #368efeFF;
  border-bottom: 1px solid #368efeFF;
}
.item-ans{
  padding: 26rpx 20rpx;
  border-bottom: 1px solid #ededed;
}
.curr_item view{
  width: 50%;
  font-size: 12px;
}
.curr_item{
  padding: 0 0 10rpx;
}
.visiro_btn{
  width: 100%;
  height: 60rpx;
  font-size: 14px;
  line-height: 60rpx;
  color: #368efeFF;
  /* border: 1px solid #368efeFF; */
}
.curr_cancel{
  color: #368efeFF;
}